{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "chat",
  "title": "Proxy Chat",
  "description": "Query ChatGPT from raycast with controls for token usage and model selection.",
  "icon": "command-icon.png",
  "author": "michael_d_angelo",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Chat",
      "description": "Ask chat something",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.70.3",
    "node-fetch": "^3.3.2",
    "openai": "^4.32.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
